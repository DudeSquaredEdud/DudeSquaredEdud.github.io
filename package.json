{
  "name": "dudesquarededud-github-io",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "lint": "eslint src --ext .js,.astro",
    "lint:fix": "eslint src --ext .js,.astro --fix",
    "format": "prettier --write src/**/*.{js,astro,json,md}",
    "format:check": "prettier --check src/**/*.{js,astro,json,md}",
    "pre-commit": "npm run lint && npm run format:check && npm run test:run"
  },
  "dependencies": {
    "astro": "^5.13.9",
    "katex": "^0.16.22"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/dom": "^10.4.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-plugin-astro": "^1.3.1",
    "fake-indexeddb": "^6.2.2",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "vitest": "^3.2.4"
  }
}
